    "12a0edfd-3555-4e0b-94a3-311d552c7304": {
      "type": "image-with-text",
      "blocks": {
        "template--21605512020279__12a0edfd-3555-4e0b-94a3-311d552c7304-heading-1": {
          "type": "heading",
